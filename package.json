{
  "name": "skillswap",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A skill exchange platform where users can trade skills instead of money",
  "keywords": [
    "skill-exchange",
    "learning",
    "community",
    "education",
    "pwa"
  ],
  "author": "SkillSwap Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && npx vite build",
    "build:production": "tsc && npx vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "npx vite preview --host 0.0.0.0 --port ${PORT:-3000}",
    "type-check": "tsc --noEmit",
    "postbuild": "echo 'Build completed successfully!'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run"
  },
